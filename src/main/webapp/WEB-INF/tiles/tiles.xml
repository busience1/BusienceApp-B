<?xml version="1.0" encoding="UTF-8"?>

<!DOCTYPE tiles-definitions PUBLIC "-//Apache Software Foundation//DTD Tiles Configuration 3.0//EN" "http://tiles.apache.org/dtds/tiles-config_3_0.dtd">

<tiles-definitions>
	
	<!-- header X, left X -->
   <definition name="normalLayout" template="/WEB-INF/views/common/normalLayout.jsp">
       <put-attribute name="body" value="" />
   </definition>
   <definition name="popup/*" extends="normalLayout">
       <put-attribute name="body" value="/WEB-INF/views/popup/{1}.jsp" />
   </definition>

   <!-- main -->
   <definition name="tilesLayout" template="/WEB-INF/views/common/tilesLayout.jsp">
       <put-attribute name="header" value="/WEB-INF/views/common/header.jsp" />
       <put-attribute name="left" value="/WEB-INF/views/common/left.jsp" />
       <put-attribute name="body" value="" />
   </definition>
   <definition name="main" extends="tilesLayout">
       <put-attribute name="body" value="/WEB-INF/views/main.jsp" />
   </definition>
   
   <definition name="*/*" extends="tilesLayout">
       <put-attribute name="body" value="/WEB-INF/views/{1}/{2}.jsp" />
   </definition>
    <definition name="*/*/*" extends="tilesLayout">
       <put-attribute name="body" value="/WEB-INF/views/{1}/{2}/{3}.jsp" />
   </definition>
   
   <definition name="work_order_TAB2" extends="normalLayout">
       <put-attribute name="body" value="/WEB-INF/views/work_order_TAB2.jsp" />
   </definition>
   
   <definition name="workMonitoring" extends="normalLayout">
       <put-attribute name="body" value="/WEB-INF/views/monitoring/workMonitoring.jsp" />
   </definition>
   
   <definition name="equipMonitoring" extends="normalLayout">
       <put-attribute name="body" value="/WEB-INF/views/monitoring/equipMonitoring.jsp" />
   </definition>
   
   <definition name="workorderoi" extends="normalLayout">
       <put-attribute name="body" value="/WEB-INF/views/monitoring/workorderoi.jsp" />
   </definition>   
   
   <definition name="work_order_TAB2" extends="normalLayout">
       <put-attribute name="body" value="/WEB-INF/views/work_order_TAB2.jsp" />
   </definition>
</tiles-definitions>